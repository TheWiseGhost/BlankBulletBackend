{
  "builds": [
    {
      "src": "blankbullet/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "installCommand": "pip install -r requirements.txt",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "blankbullet/wsgi.py"
    }
  ]
}
